{
    "workbench.iconTheme": "vscode-icons",
    "editor.mouseWheelZoom": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Default",
    "explorer.confirmDelete": false,
    "workbench.activityBar.orientation": "vertical",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/*.class": true,
        "**/__pycache__/": true
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.defaultLanguage": "cpp",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": ["comment", "punctuation.definition.comment"],
                "settings": {
                    "fontStyle": "italic ",
                    "foreground": "#4c9a2a"
                }
            }
        ]
    },
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${appName} OM ",
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#11f505",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "C_Cpp.errorSquiggles": "enabled",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "jupyter.askForKernelRestart": false,
    "editor.codeActionsOnSave": {},
    "git.suggestSmartCommit": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmDragAndDrop": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.linkedEditing": true,
    "C_Cpp.default.enableConfigurationSquiggles": false,
    "code-runner.runInTerminal": true,
    "prettier.tabWidth": 4,
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "better-comments.highlightPlainText": true,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "notebook.lineNumbers": "on",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "githubPullRequests.createOnPublishBranch": "never",
    "update.mode": "manual",
    "thunder-client.htmlView": "Preview",
    "extensions.autoCheckUpdates": false,
    "npm.keybindingsChangedWarningShown": true,

    "vsicons.dontShowNewVersionMessage": true,
    "tabnine.experimentalAutoImports": true,
    "[python]": {},
    "mdb.confirmRunAll": false,
    "code-runner.terminalRoot": "/",
    "editor.fontFamily": "JetBrainsMono Nerd Font", // "JetBrains Mono"
    // "editor.fontFamily": "Source Code Pro",
    // "editor.fontFamily": "Fira Code", // Font Family
    "editor.fontWeight": "300",
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "editor.fontSize": 12,
    "editor.lineHeight": 24,
    "explorer.decorations.badges": false,
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "boot-java.rewrite.reconcile": true,
    "redhat.telemetry.enabled": true,
    "workbench.editor.empty.hint": "hidden",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "extensions.autoUpdate": false,
    "amazonQ.telemetry": false,
    "explorer.confirmPasteNative": false,
    "amazonQ.suppressPrompts": {
        "codeWhispererConnectionExpired": true
    },
    "security.promptForLocalFileProtocolHandling": false,
    "codium.codeCompletion.enable": false,
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "editor.accessibilityPageSize": 12,
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "debug.javascript.autoAttachFilter": "disabled",
    "editor.renderWhitespace": "none",
    "[pip-requirements]": {},
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "remote.SSH.remotePlatform": {
        "135.13.8.201": "linux",
        "20.41.249.65": "linux"
    },
    "markdown-preview-enhanced.previewTheme": "atom-dark.css",
    "roo-cline.allowedCommands": ["*"],
    "roo-cline.deniedCommands": [],
    "database-client.autoSync": true,
    "editor.formatOnSave": true,
    "kiroAgent.modelSelection": "claude-sonnet-4.5",
    "kiroAgent.trustedCommands": [
        "venv\\Scripts\\activate & python pyinstxtractor.py main-gui.exe",
        "node --version",
        "npm --version",
        "*"
    ],
    "kiroAgent.enableCodebaseIndexing": true,
    "kiroAgent.trustedTools": ["webFetch", "remote_web_search"],
    "kiroAgent.agentAutonomy": "Autopilot",
    "terminal.integrated.env.windows": {
        "MSYS_NO_PATHCONV": "1"
    },
    "code-runner.clearPreviousOutput": true,
    "code-runner.fileDirectoryAsCwd": false,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.ignoreSelection": true,
    "code-runner.executorMap": {
        "javascript": "node $fileName",
        "java": "javac $fileName ; java $fileNameWithoutExt",
        "c": "gcc $fileName -o $fileNameWithoutExt ; .\\$fileNameWithoutExt",
        "cpp": "g++ $fileName -o $fileNameWithoutExt ; .\\$fileNameWithoutExt",
        "python": "python -u $fileName",
        "go": "go run $fileName",
        "bat": "cmd /c $fileName",
        "shellscript": "bash $fileName",
        "typescript": "ts-node $fileName"
    }
}
